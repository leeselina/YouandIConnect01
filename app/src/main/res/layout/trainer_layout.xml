<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/trainerLayout">

    <EditText
        android:layout_width="131dp"
        android:layout_height="27dp"
        android:inputType="textPersonName"
        android:text="SEOUL"
        android:ems="10"
        tools:layout_editor_absoluteY="196dp"
        tools:layout_editor_absoluteX="136dp"
        android:id="@+id/expert_address" />

    <EditText
        android:layout_width="131dp"
        android:layout_height="27dp"
        android:text="SEOUL"
        android:ems="10"
        tools:layout_editor_absoluteY="157dp"
        tools:layout_editor_absoluteX="136dp"
        android:id="@+id/expert_certification_number"
        android:inputType="number" />

    <EditText
        android:layout_width="131dp"
        android:layout_height="27dp"
        android:text="SEOUL"
        android:ems="10"
        tools:layout_editor_absoluteY="74dp"
        tools:layout_editor_absoluteX="128dp"
        android:id="@+id/expert_resident_number"
        android:inputType="number" />

    <EditText
        android:layout_width="131dp"
        android:layout_height="27dp"
        android:inputType="textPersonName"
        android:text="Name"
        android:ems="10"
        tools:layout_editor_absoluteY="32dp"
        tools:layout_editor_absoluteX="136dp"
        android:id="@+id/expert_name" />

    <TextView
        android:text="본명"
        android:layout_width="101dp"
        android:layout_height="27dp"
        tools:layout_editor_absoluteY="32dp"
        tools:layout_editor_absoluteX="11dp"
        android:id="@+id/textView2" />

    <TextView
        android:text="자격증번호"
        android:layout_width="101dp"
        android:layout_height="27dp"
        tools:layout_editor_absoluteY="157dp"
        tools:layout_editor_absoluteX="11dp"
        android:id="@+id/textView3" />

    <TextView
        android:text="활동지역"
        android:layout_width="101dp"
        android:layout_height="27dp"
        tools:layout_editor_absoluteY="196dp"
        tools:layout_editor_absoluteX="11dp"
        android:id="@+id/textView6" />

    <TextView
        android:text="한줄 소개"
        android:layout_width="101dp"
        android:layout_height="27dp"
        tools:layout_editor_absoluteY="342dp"
        tools:layout_editor_absoluteX="11dp"
        android:id="@+id/textView8" />

    <TextView
        android:text="약력"
        android:layout_width="101dp"
        android:layout_height="27dp"
        tools:layout_editor_absoluteY="231dp"
        tools:layout_editor_absoluteX="11dp"
        android:id="@+id/textView7" />

    <TextView
        android:text="담당분야/세부분야"
        android:layout_width="117dp"
        android:layout_height="27dp"
        tools:layout_editor_absoluteY="117dp"
        tools:layout_editor_absoluteX="11dp"
        android:id="@+id/textView5" />

    <TextView
        android:text="주민등록번호"
        android:layout_width="101dp"
        android:layout_height="27dp"
        tools:layout_editor_absoluteY="75dp"
        tools:layout_editor_absoluteX="11dp"
        android:id="@+id/textView4" />

    <EditText
        android:layout_width="136dp"
        android:layout_height="34dp"
        android:inputType="date"
        android:ems="10"
        tools:layout_editor_absoluteY="338dp"
        tools:layout_editor_absoluteX="133dp"
        android:id="@+id/expert_description" />

    <Spinner
        android:layout_width="109dp"
        android:layout_height="30dp"
        tools:layout_editor_absoluteY="117dp"
        tools:layout_editor_absoluteX="251dp"
        android:id="@+id/spinner" />

    <Spinner
        android:layout_width="86dp"
        android:layout_height="30dp"
        tools:layout_editor_absoluteY="117dp"
        tools:layout_editor_absoluteX="143dp"
        android:id="@+id/spinner2" />

    <EditText
        android:layout_width="235dp"
        android:layout_height="94dp"
        android:inputType="textMultiLine"
        android:ems="10"
        tools:layout_editor_absoluteY="233dp"
        tools:layout_editor_absoluteX="133dp"
        android:id="@+id/expert_brief_history" />

</android.support.constraint.ConstraintLayout>